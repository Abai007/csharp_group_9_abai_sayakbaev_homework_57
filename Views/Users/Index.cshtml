@model List<User>
@{ ViewData["Title"] = "Users";

                Layout = "_Layout"; }
<h2>Users</h2>
<a asp-action="Register" asp-controller="Account">Добавить пользователя</a>
<div>
    @if (@Model.Count == 0)

    {

<p>Order not yet added</p>}

    <table class="table">


        <tr>

            <th>Id</th>
            <th>Email</th>
            <th>Edit</th>


        </tr>

        @foreach (var item in @Model)

        {

    <tr>
        <td>@item.Id</td>
        <td>@item.Email</td>
        <td> <button><a asp-action="Editing" asp-controller="Users" asp-route-id="@item.Id">Подробнее</a></button></td>
    </tr>
    }
    </table>


</div>
